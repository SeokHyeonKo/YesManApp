<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".interestAcitivity">

    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/domain0"
        android:src="@drawable/bestseller_circle"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />
    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/domain1"
        android:src="@drawable/bestseller_circle"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />
    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/domain2"
        android:src="@drawable/bestseller_circle"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true" />
    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/domain3"
        android:src="@drawable/bestseller_circle"
        android:layout_alignTop="@+id/domain4"
        android:layout_alignParentStart="true" />
    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/domain4"
        android:src="@drawable/bestseller_circle"
        android:layout_below="@+id/domain1"
        android:layout_alignStart="@+id/domain1"
        android:layout_marginTop="39dp" />
    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/domain5"
        android:src="@drawable/bestseller_circle"
        android:layout_alignTop="@+id/domain4"
        android:layout_alignStart="@+id/domain2" />
    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/domain6"
        android:src="@drawable/bestseller_circle"
        android:layout_alignTop="@+id/domain7"
        android:layout_alignParentStart="true" />
    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/domain7"
        android:src="@drawable/bestseller_circle"
        android:layout_alignTop="@+id/domain8"
        android:layout_alignStart="@+id/domain4" />
    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/domain8"
        android:src="@drawable/bestseller_circle"
        android:layout_below="@+id/domain5"
        android:layout_alignEnd="@+id/scrollView"
        android:layout_marginTop="39dp" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/domain_c0"
        android:checked="false"
        android:layout_alignTop="@+id/domain0"
        android:layout_alignEnd="@+id/domain0" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/domain_c1"
        android:checked="false"
        android:layout_alignTop="@+id/domain1"
        android:layout_alignEnd="@+id/domain1" />
    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/domain_c2"
        android:checked="false"
        android:layout_alignTop="@+id/domain2"
        android:layout_alignEnd="@+id/domain2" />
    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/domain_c3"
        android:checked="false"
        android:layout_alignTop="@+id/domain3"
        android:layout_alignEnd="@+id/domain3" />
    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/domain_c4"
        android:checked="false"
        android:layout_alignTop="@+id/domain4"
        android:layout_alignEnd="@+id/domain4" />
    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/domain_c5"
        android:checked="false"
        android:layout_alignTop="@+id/domain5"
        android:layout_alignEnd="@+id/domain5" />
    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/domain_c6"
        android:checked="false"
        android:layout_alignTop="@+id/domain6"
        android:layout_alignEnd="@+id/domain6" />
    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/domain_c7"
        android:checked="false"
        android:layout_alignTop="@+id/domain7"
        android:layout_alignEnd="@+id/domain7" />
    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/domain_c8"
        android:checked="false"
        android:layout_alignTop="@+id/domain8"
        android:layout_alignEnd="@+id/domain8" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="확 인"
        android:id="@+id/button"
        android:layout_alignParentBottom="true"
        android:layout_alignEnd="@+id/domain8"
        android:layout_alignParentStart="true"
        android:onClick="Onclick_next" />

    <ScrollView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/scrollView"
        android:layout_below="@+id/domain_c5"
        android:layout_alignParentEnd="true" />
</RelativeLayout>
